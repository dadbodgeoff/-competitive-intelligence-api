# Development Environment Variables
# Copy this to .env.dev and fill in your actual values

# Supabase Configuration (DEV)
SUPABASE_URL=https://your-dev-project.supabase.co
SUPABASE_ANON_KEY=your-dev-anon-key
SUPABASE_SERVICE_KEY=your-dev-service-key
SUPABASE_SERVICE_ROLE_KEY=your-dev-service-role-key

# JWT Configuration
JWT_SECRET_KEY=generate-a-secure-random-key-here
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Application Configuration
APP_ENV=development
ENVIRONMENT=development
APP_PORT=8000
APP_HOST=localhost

# External API Keys
GOOGLE_PLACES_API_KEY=your-google-places-api-key
GOOGLE_GEMINI_API_KEY=your-gemini-api-key

# Vertex AI for Image Generation (Imagen 3.0)
VERTEX_AI_API_KEY=your-vertex-ai-key
VERTEX_AI_PROJECT_ID=your-gcp-project-id
VERTEX_AI_LOCATION=us-central1
CREATIVE_ASSETS_BUCKET=creative-assets

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# ClamAV Configuration
CLAMAV_HOST=clamav
CLAMAV_PORT=3310

# Outscraper API
OUTSCRAPER_API_KEY=your-outscraper-key

# SerpAPI
SERPAPI_KEY=your-serpapi-key

# Google Custom Search
GOOGLE_CUSTOM_SEARCH_ENGINE_ID=your-search-engine-id

# Nano Banana (if using external service)
NANO_BANANA_API_KEY=your-nano-banana-key
NANO_BANANA_WEBHOOK_SECRET=your-webhook-secret
