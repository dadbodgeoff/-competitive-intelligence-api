{
  "debounce_seconds": 2,
  "backend_port": 8000,
  "frontend_port": 5173,
  "watch_extensions": [".py", ".js", ".ts", ".tsx", ".json", ".sql", ".md"],
  "ignore_patterns": [
    "__pycache__",
    ".git",
    "node_modules",
    ".vscode",
    "dist",
    "build",
    ".pytest_cache",
    "coverage"
  ],
  "backend_command": ["python", "-m", "uvicorn", "api.main:app", "--reload", "--port", "8000"],
  "frontend_command": ["npm", "run", "dev"],
  "frontend_directory": "frontend"
}